{
  "template": {
    "CSS": [
      "* {",
      "  color: {{ white }};",
      "  background-color: {{ background }};",
      "  outline: none;",
      "}",
      "",
      "QLineEdit:focus,",
      "QCombobox:focus,",
      "QCheckBox:focus,",
      "QPushButton:focus {",
      "  background-color: {{ backgroundFocus }};",
      "}",
      "",
      "*:disabled {",
      "  color: {{ whiteDisabled }};",
      "}",
      "",
      "*:pressed {",
      "  background-color: {{ primary }};",
      "}"
    ],
    "Toolbar": [
      "QToolBar {",
      "  background-color: {{ backgroundHi }};",
      "}",
      "",
      "",
      "QToolBar::separator {",
      "    background-color: {{ transparentWhite }};",
      "    margin: 2px;",
      "}",
      "",
      "QToolButton {",
      "    background-color: transparent;",
      "}",
      "",
      "QToolButton:hover {",
      "    background-color: {{ backgroundHover }};",
      "}",
      "",
      "",
      "",
      "",
      "",
